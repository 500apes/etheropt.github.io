<% filtered_options = options.filter(function(x){return x.contract_addr==contract.contract_addr}) %>
<nav>
  <div class="navbar-form navbar-left">
    <button type="button" class="btn btn-default" onclick="bundle.Main.refresh();"><i class="fa fa-refresh"></i> Refresh</button>
  </div>
  <div class="navbar-form navbar-left">
    <button type="button" class="btn btn-default" onclick="window.open('<%= contract.contract_link %>');"><i class="fa fa-file-text"></i> Smart contract</button>
  </div>
  <div class="navbar-form navbar-left">
    <button type="button" class="btn btn-default" data-toggle="modal" data-target="#fund_modal" data-contract="<%= contract.contract_addr%>">Funds <span class="badge"><%= bundle.utility.weiToEth(contract.funds) %> eth</span></button>
  </div>
  <div class="navbar-form navbar-left">
    <button type="button" class="btn btn-default" data-toggle="modal" data-target="#withdraw_modal" data-contract="<%= contract.contract_addr%>">Available <span class="badge"><%= bundle.utility.weiToEth(contract.fundsAvailable) %> eth</span></button>
  </div>
  <div class="navbar-form navbar-left">
    <button type="button" class="btn btn-default" data-toggle="modal" data-target="#expire_modal" data-contract="<%= contract.contract_addr%>"><i class="fa fa-calendar"></i> Expire</button>
  </div>
</nav>
